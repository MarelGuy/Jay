{
    "name": "jay-language-support",
    "displayName": "Jay Language Support",
    "description": "Jay language support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jay",
            "aliases": ["Jay", "jay"],
            "extensions": [".jay"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "jay",
            "scopeName": "source.jay",
            "path": "./syntaxes/jay.tmLanguage.json"
        }]
    }
}